{
    "name": "shippinno/email",
    "type": "library",
    "repositories": [
        {
            "type": "vcs",
            "url": "git@github.com:shippinno/value-objects-php.git",
            "no-api": true
        }
    ],
    "require": {
        "php": ">=7.1",
        "symfony/mailer": "^5.3|^6.0",
        "symfony/dependency-injection": "^5.3|^6.0",
        "tanigami/value-objects": "dev-develop",
        "psr/log": "^1.0|^2.0|^3.0",
        "egulias/email-validator": "^2.1|^3.0|^4.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.2",
        "phpunit/phpunit": "^10.1"
    },
    "autoload": {
        "psr-4": {
            "Shippinno\\Email\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Shippinno\\Email\\": "tests/"
        }
    }
}
